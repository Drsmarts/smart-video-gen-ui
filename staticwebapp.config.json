{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/assets/*", "/images/*", "/css/*", "/js/*"]
  },
  "routes": [
    { "route": "/api/*", "allowedRoles": ["anonymous"] }
  ],
  "responseOverrides": {
    "404": { "rewrite": "/index.html" }
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}
